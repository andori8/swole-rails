<% if flash[:alert] %>
  <% if flash[:alert].include?("deleted") || flash[:alert].include?("Can't") %>
    <div class="alert">
      <strong><%= flash[:alert] %></strong>
    </div>
  <% else %>
    <div class="alert success">
      <strong><%= flash[:alert] %></strong>
    </div>
  <% end %>
<% end %>
